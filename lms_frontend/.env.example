# Frontend URLs
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_BACKEND_URL=http://localhost:8080

# Feature flags (JSON or comma-separated list)
# Example JSON enabling Supabase browser mode:
# REACT_APP_FEATURE_FLAGS='{"FLAG_SUPABASE_MODE":true}'
REACT_APP_FEATURE_FLAGS=

# Optional experiments toggle
REACT_APP_EXPERIMENTS_ENABLED=false

# Health endpoint path (defaults to /health)
REACT_APP_HEALTHCHECK_PATH=/health

# Logging level: debug | info | warn | error
REACT_APP_LOG_LEVEL=info

# Supabase browser mode configuration (used only when FLAG_SUPABASE_MODE is enabled)
REACT_APP_SUPABASE_URL=
# Preferred env var for anon key:
REACT_APP_SUPABASE_KEY=
# Legacy alias supported by code (either one may be set):
REACT_APP_SUPABASE_ANON_KEY=

# Version (optional)
REACT_APP_VERSION=0.1.0
