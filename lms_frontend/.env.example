# Frontend environment example
# Copy to .env and set values appropriately.

# App
REACT_APP_NODE_ENV=development
REACT_APP_PORT=3000
REACT_APP_LOG_LEVEL=info
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_FEATURE_FLAGS={"FLAG_SUPABASE_MODE":true}

# Supabase (required when FLAG_SUPABASE_MODE is true)
REACT_APP_SUPABASE_URL=https://your-project.supabase.co
REACT_APP_SUPABASE_KEY=your-public-anon-key

# Optional legacy backend (not used when Supabase mode is true)
REACT_APP_API_BASE=http://localhost:8080
REACT_APP_BACKEND_URL=http://localhost:8080
REACT_APP_WS_URL=ws://localhost:8080
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_TRUST_PROXY=false
REACT_APP_EXPERIMENTS_ENABLED=false
